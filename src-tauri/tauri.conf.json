{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "pydio-cells-synchronizer",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "Cells Synchronizer",
        "width": 1200,
        "height": 800,
        "minWidth": 650,
        "minHeight": 400
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.iewnfod.pydio.cells.synchronizer",
      "icon": [
        "icons/icon.icns"
      ]
    }
  }
}
